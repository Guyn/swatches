{
	"name": "swatches",
	"version": "1.0.0",
	"description": "Swatches",
	"author": "Sil van Diepen",
	"private": true,
	"scripts": {
		"dev": "sh scripts/dev.sh",
		"dev:local": "sh scripts/devlocal.sh",
		"dev:tst": "sh scripts/tst.sh",
		"dev:prd": "sh scripts/prd.sh",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
		"precommit": "lint && stylelint",
		"generate": "nuxt generate",
		"generate:develop": "cp ./env/.env.dev ./.env && nuxt generate",
		"generate:staging": "cp ./env/env.tst ./.env && nuxt generate",
		"generate:production": "cp ./env/.env.prd ./.env && nuxt generate",
		"stylelint": "stylelint '**/*.vue' '**/*.scss'",
		"stylelint:fix": "stylelint '**/*.vue' '**/*.scss' --fix"
	},
	"dependencies": {
		"@guyn/convert": "^1.4.6",
		"@nuxt/typescript-runtime": "^0.4.0",
		"@nuxtjs/axios": "^5.4.1",
		"@nuxtjs/google-analytics": "^2.2.0",
		"@nuxtjs/pwa": "^3.0.0-0",
		"nuxt": "^2.8.1",
		"stylelint-logical-order": "0.0.8"
	},
	"devDependencies": {
		"@guyn/color": "*",
		"@henris/custom": "^0.0.2",
		"@nuxt/typescript-build": "^0.6.0",
		"@nuxtjs/dotenv": "^1.4.1",
		"@nuxtjs/eslint-config-typescript": "^1.0.0",
		"@nuxtjs/eslint-module": "^1.0.0",
		"@nuxtjs/stylelint-module": "^3.1.0",
		"@vue/test-utils": "^1.0.0-beta.27",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.1.0",
		"eslint": "^6.1.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-nuxt": ">=0.4.2",
		"eslint-plugin-prettier": "^3.1.2",
		"henris": "^1.2.0",
		"husky": "^4.0.0",
		"jest": "^24.1.0",
		"lint-staged": "^10.0.0",
		"nuxt-favicons": "^0.0.2",
		"prettier": "^1.19.1",
		"sass": "^1.22.9",
		"sass-loader": "^8.0.2",
		"stylelint": "^10.1.0",
		"stylelint-order": "^2.2.0",
		"ts-jest": "^25.0.0",
		"vue-jest": "^4.0.0-0"
	}
}
